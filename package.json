{
  "name": "api-integration",
  "version": "0.1.0",
  "bin": {
    "api-integration": "bin/api-integration.js"
  },
  "scripts": {
    "up": "npm run build && npm run debug && npm run deploy",
    "dev": "npm run build && npm run localstack && npm run local:deploy",
    "debug": "npm run build && npm run debug:wikiPageHandler",
    "debug:wikiPageHandler": "sam local invoke wikiPageHandlerFF0F0999 -e debug/event.json > debug/output.json",
    "deploy": "npm run cdk deploy",
    "build": "npm run build:TypeScript && npm run build:CloudFormation && npm run lint",
    "build:TypeScript": "tsc",
    "build:CloudFormation": "cdk synth --no-staging > template.yml",
    "lint": "tslint -c tslint.json --fix 'src/*.ts'",
    "test": "jest",
    "cdk": "cdk"
  },
  "devDependencies": {
    "@aws-cdk/assert": "^1.19.0",
    "@types/jest": "^24.0.22",
    "@types/node": "10.17.5",
    "aws-cdk": "^1.19.0",
    "aws-lambda-test-utils": "^1.3.0",
    "http": "0.0.0",
    "jest": "^24.9.0",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.1.0",
    "tslint": "^5.20.1",
    "typescript": "~3.7.2"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "^1.19.0",
    "@aws-cdk/aws-lambda": "^1.19.0",
    "@aws-cdk/aws-logs": "^1.19.0",
    "@aws-cdk/aws-logs-destinations": "^1.19.0",
    "@aws-cdk/core": "^1.19.0",
    "@types/aws-lambda": "^8.10.39",
    "source-map-support": "^0.5.16"
  }
}
